<!--<script type="text/javascript">

var offset = 0;
var featured = 0;
var author = 0;
var featuredNames = [];
var deckGrid = 0;
const latestDecks = document.querySelector('.deck-list');

jQuery( document ).ready(function() {
    const loadFromURLPair = (function(value, key) {
        //Name
        if(key == "name"){
           jQuery('#deckName').val(value);
        }
        //Card Code
         if(key == "cardcode"){
             var codecount = 1;
             var codeArray = value.split('|');
             codeArray.pop();
             jQuery.each(codeArray, function( index, value ) {
                jQuery('#DSPcardCode'+codecount).css('display', 'block').val(value);
                jQuery('#addCodeButton'+codecount).remove();
                jQuery('#addCodeButton'+(codecount + 1)).show();
                codecount++;
             });
        }
        //Deck Archetype
        if(key == "archetype"){
            jQuery('#deckArchetype').val(value);
        }
        //Sort
        if(key == "sort"){
           jQuery('#filter-sort').val(value);
        }
        //Color
        if(key == "color"){
           jQuery('#filter-color').val(value).change();
        }
        //Format
        if(key == "format"){
            valuemod = value.replace(/-/g, ' ')
           jQuery('#filter-format').val((valuemod).toLowerCase());
        }
        //Format
        if(key == "_sft_category"){
            jQuery('#filter-format').val(value);
        }
        //Deck Size
        if(key == "decksize"){
           jQuery('#filter-decksize').val(value);
        }
        //Archetype
        if(key == "_sft_post_tag"){
           jQuery('#filter-archetype').val((value).toLowerCase()).change();
        }
        //Tournament
        if(key == "tournament"){
           if (value === 'Yes') value = 'tier-2'; /* backwards compatibility */
           jQuery('#filter-tournamentdeck').val(value);
        }
        //From Date
        if(key == "from"){
           jQuery('#from-date').val(value).change();
        }
        //To Date
        if(key == "to"){
           jQuery('#to-date').val(value).change();
        }
        //Author
        if(key == "author"){
           author = value;
           jQuery('#filter-author').val((author).toLowerCase()).change();
        }
        //Price Classification
        if(key == "price"){
           jQuery('#filter-priceclass').val(value).change();
        }
        //From Price
        if(key == "fromprice"){
           jQuery('#from-price').val(value).change();
        }
        //To Price
        if(key == "toprice"){
           jQuery('#to-price').val(value).change();
        }
        //Deck Range
        if(key == "range"){
           jQuery('#filter-deckrange').val(value);
        }
        //Banlist
        if(key == "banlist"){
              jQuery('#filter-banlistcheck').prop('checked', true);
        }
        //Featured Deck Builder
        if(key == "featuredBuilders"){
           featured = 1;
        }
    });
    
    //Foreach query string param - Set the Relevant Input Value
    if (window.location.search) { // backwards compatibility
        (new URLSearchParams(window.location.search.substr(1))).forEach(loadFromURLPair);
    }
    if (window.location.hash) { // more GET friendly
        (new URLSearchParams(window.location.hash.substr(1))).forEach(loadFromURLPair);
    }


    searchDecks(0);

    jQuery('.loading').hide();

    jQuery('.deck-maindeck').show();

    //Autocomplete - Card Name
    jQuery('.contains-card').autocomplete({
        serviceUrl: '/api/autocomplete.php',
        deferRequestBy: 300,
        onSelect: function (suggestion) {
           jQuery(this).val(suggestion.name);
           searchDecks(0);
        },
        formatResult: function (suggestion, currentValue) {
            var defaultImageUrl = suggestion.default_image;
            //Build HTML
            var suggestionHTML = '<div class="suggestion-item d-flex align-items-center justify-content-between">';
            var imageUrl = suggestion.image && suggestion.image.trim() !== "" ? suggestion.image : defaultImageUrl;
            suggestionHTML += '<div><img src="' + imageUrl + '" onerror="this.onerror=null;this.src=\'' + defaultImageUrl + '\';" class="rounded suggestion-image mr-2" loading="lazy" />' + suggestion.value + '</div>';
            if (suggestion.image_type && suggestion.image_type.trim() !== "") {
                suggestionHTML += '<img src="' + suggestion.image_type + '" class="rounded suggestion-image-type ml-auto" loading="lazy" />';
            }
            if (suggestion.image_race && suggestion.image_race.trim() !== "") {
                suggestionHTML += '<img src="' + suggestion.image_race + '" class="rounded suggestion-image-race ml-2" loading="lazy" />';
            }
            suggestionHTML += '</div>';
            return suggestionHTML;
        }
    });

    jQuery('.does-not-contain').autocomplete({
        serviceUrl: '/api/autocomplete.php',
        onSelect: function (suggestion) {
           jQuery(this).val(suggestion.name);
           searchDecks(0);
        },
        formatResult: function (suggestion, currentValue) {
            //Build Autocomplete HTML
            var suggestionHTML = '<div class="suggestion-item d-flex align-items-center justify-content-between">';
            suggestionHTML += '<div><img src="' + suggestion.image + '" class="rounded suggestion-image mr-2" loading="lazy" />' + suggestion.value + '</div>';
            if (suggestion.image_type && suggestion.image_type.trim() !== "") {
                suggestionHTML += '<img src="' + suggestion.image_type + '" class="rounded suggestion-image-type ml-auto" loading="lazy" />';
            }
            if (suggestion.image_race && suggestion.image_race.trim() !== "") {
                suggestionHTML += '<img src="' + suggestion.image_race + '" class="rounded suggestion-image-race ml-2" loading="lazy" />';
            }
            suggestionHTML += '</div>';
            return suggestionHTML;
        }
    });

    //Autocomplete - Deck Archetype
    jQuery('#filter-archetype').autocomplete({
        serviceUrl: '/api/autocompleteDeckArchetype.php',
        onSelect: function (suggestion) {
           jQuery('#deckArchetype').val(suggestion.name);
           //alert('You selected: ' + suggestion.value + ', ' + suggestion.data);
        }
    });

    //Autocomplete - User Search
    jQuery('#filter-author').autocomplete({
        serviceUrl: '/api/users/usersAutoCompletePub.php',
        onSelect: function (suggestion) {
            jQuery('#filter-author').val(suggestion.data);
        }
    });

    // Check certain inputs (select & input tags) in filter panel on change event for any value
    jQuery('#filter-panel .deck-search-form-select, #from-date, #to-date, #filter-banlistcheck').on('change', function () {
        let isDisabled = true;

        // iterate through all select dropdowns inside filter panel
        jQuery('#filter-panel .deck-search-form-select').each(function () {
            if (jQuery(this).prop('selectedIndex') > 0) {
                isDisabled = false;
            }
        });

        // iterate through input dates inside filter panel
        jQuery('#from-date, #to-date').each(function () {
            if (jQuery(this).val() !== '') {
                isDisabled = false;
            }
        });

        // check if checkbox inside filter panel is checked
        if (jQuery('#filter-banlistcheck').prop('checked')) {
            isDisabled = false;
        }
        
        if (isDisabled) {
            jQuery('#resetFilters').prop('disabled', true).addClass('btn-disabled');
            if (typeof(Storage) !== 'undefined') {
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'false');
            }
        } else {
            jQuery('#resetFilters').prop('disabled', false).removeClass('btn-disabled');
            if (typeof(Storage) !== 'undefined') {
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'true');
            }
        }
    });

    // Same as above but on input event for the other inputs
    jQuery('.contains-card, .does-not-contain, #filter-archetype, #filter-author, #from-price, #to-price').on('input', function () {
        jQuery('.contains-card, .does-not-contain, #filter-archetype, #filter-author, #from-price, #to-price').each(function () {
            if (jQuery(this).val() !== '') {
                jQuery('#resetFilters').prop('disabled', false).removeClass('btn-disabled');
                if (typeof(Storage) !== 'undefined') {
                    sessionStorage.setItem('HasDeckSearchFiltersSet', 'true');
                }
                return false;
            } else {
                jQuery('#resetFilters').prop('disabled', true).addClass('btn-disabled');
                if (typeof(Storage) !== 'undefined') {
                    sessionStorage.setItem('HasDeckSearchFiltersSet', 'false');
                }
            }
        });
    });

    // Search decks when pressing enter in search input
    const searchInput = document.getElementById('deckName');
    searchInput.addEventListener('keyup', function (evt) {
        if (evt.key === 'Enter') {
            searchDecks(0);
        }
    })
});

//Search Decks
jQuery("#searchDecks").click(function(){
    searchDecks(0);
});

//Query String Variable
function getQueryVariable(variable)
{
   var query = window.location.search.substring(1);
   var vars = query.split("&");
   for (var i=0;i<vars.length;i++) {
           var pair = vars[i].split("=");
           if(pair[0] == variable){return pair[1];}
   }
   return(null);
}

function htmlEntities(str) {
    return String(str).replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/"/g, '&quot;');
}

function checkSearchSortFilterValues(conditions) {
    let notEmptyString = [
        conditions.deckName,
        conditions.doesContain,
        conditions.doesNotContain,
        conditions.archetype,
        conditions.author,
        conditions.fromDate,
        conditions.toDate,
        conditions.fromPrice,
        conditions.toPrice
    ].some(condition => condition !== '') ? true : false;

    if (
        notEmptyString ||
        conditions.format !== 'Format' ||
        conditions.tournament !== 'any' ||
        conditions.deckSize !== '0' ||
        conditions.sort !== 'Sort By' ||
        conditions.range !== 'all' ||
        conditions.banlistCheck
    ) return true;

    return false;
}

function searchDecks(offset){

    //If First Page of Decks or loading
    jQuery('.nextDeck').addClass("disabled").removeAttr("onclick").off("click").empty().append('<span class="page-link pagelink-btn">Previous</a>');
    jQuery('.prevDeck').addClass("disabled").removeAttr("onclick").off("click").empty().append('<span class="page-link pagelink-btn">Next</a>');

     //Get Decks
    var apiurl = "/api/decks/getDecks.php?";

    //Check if Name is Set
    var deckname = jQuery('#deckName').val();
    deckname = deckname.trim().replace(/\s+/g, " "); // remove extra whitespace
    jQuery('#deckName').val(deckname);
    if(deckname !== ''){apiurl += "&name="+deckname;}

    //Check if Contains Card Inputs are set
    var cardcodeurl = '';
    jQuery('.contains-card').each(function(i, obj) {
         var cardCode = jQuery(obj).val();
         if(cardCode !== ''){
            cardcodeurl += encodeURIComponent(cardCode)+"%7C";
         }
    });
    if(cardcodeurl !== ''){
        cardcodeurl = cardcodeurl.replace(/,\s*$/, "");
        apiurl += "&cardcode="+cardcodeurl;
    }

    //Check if does not contain card inputs are set
    var notcardcodeurl = '';
    jQuery('.does-not-contain').each(function(i, obj) {
        var cardCode = jQuery(obj).val();
        if(cardCode !== ''){
            notcardcodeurl += encodeURIComponent(cardCode)+"%7C";
        }
    });
    if(notcardcodeurl !== ''){
        notcardcodeurl = notcardcodeurl.replace(/,\s*$/, "");
        apiurl += "&notcardcode="+notcardcodeurl;
    }

    //Check if Archetype is set
    //var deckarchetype = jQuery('#deckArchetype').val();
    //if(deckarchetype !== ''){apiurl += "&archetype="+deckarchetype;}

    //Check if Format is Set
    var cardFormat = jQuery('#filter-format').val();
    if(cardFormat !== 'Format'){apiurl += "&_sft_category="+cardFormat;}

    //Check if Tournament is Set
    var cardTournament = jQuery('#filter-tournamentdeck').val() || 'any';
    if(cardTournament !== 'any'){apiurl += "&tournament="+cardTournament;}

    //Check if Deck Size is Set
    var decksize = jQuery('#filter-decksize').val();
    if(decksize !== '0'){apiurl += "&decksize="+decksize;}

    //Check if Deck Archetype is Set
    var deckarchetype = jQuery('#filter-archetype').val();
    if(deckarchetype !== ''){apiurl += "&_sft_post_tag="+deckarchetype;}

    //Check if Author is Set
    var deckauthor = jQuery('#filter-author').val();
    if(deckauthor !== ''){apiurl += "&author="+deckauthor;}

    //Check if Sort is set
	var deckSort = jQuery('#filter-sort').val();
	if(deckSort != "Sort By"){ apiurl+= "&sort="+deckSort; }

	//Check From Date is set
	var fromdate = jQuery('#from-date').val();
	if(fromdate !== ''){ apiurl+= "&from="+fromdate; }

	//Check To Date is set
	var todate = jQuery('#to-date').val();
	if(todate !== ''){ apiurl+= "&to="+todate; }

	//Check From Price is set
	var fromprice = jQuery('#from-price').val();
	if(fromprice !== ''){ apiurl+= "&fromprice="+fromprice; }

	//Check To Price is set
	var toprice = jQuery('#to-price').val();
	if(toprice !== ''){ apiurl+= "&toprice="+toprice; }

    //Check if Deck Range is Set
    var deckrange = jQuery('#filter-deckrange').val();
    if(deckrange !== 'all'){apiurl += "&range="+deckrange;}

    //If filter-banlist is checked then add banlist to apiurl
    if(jQuery('#filter-banlistcheck').is(':checked')){
        apiurl += "&banlist";
    }

    //Check Author is set
	// if(author !== 0){ apiurl+= "&author="+author; }

	//Check if Featured Deck Builder is set
	if(featured == 1){ apiurl+= "&featuredBuilders"; }

    // Iterate through all inputs (select & input tags) inside filter panel then check if any one of them has a value
    jQuery('#filter-panel .deck-search-form-select, #filter-panel .input-border, #filter-banlistcheck').each(function () {
        if (jQuery(this)[0].tagName === 'SELECT') {
            if (jQuery(this).prop('selectedIndex') > 0) {
                jQuery('#resetFilters').prop('disabled', false).removeClass('btn-disabled');
                jQuery('#filter-btn').addClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'true');
                return false;
            } else {
                jQuery('#resetFilters').prop('disabled', true).addClass('btn-disabled');
                jQuery('#filter-btn').removeClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'false');
            }
        } else if (jQuery(this)[0].tagName === 'INPUT' && jQuery(this)[0].type !== 'checkbox') {
            if (jQuery(this).val() !== '') {
                jQuery('#resetFilters').prop('disabled', false).removeClass('btn-disabled');
                jQuery('#filter-btn').addClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'true');
                return false;
            } else {
                jQuery('#resetFilters').prop('disabled', true).addClass('btn-disabled');
                jQuery('#filter-btn').removeClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'false');
            }
        } else if (jQuery(this)[0].tagName === 'INPUT' && jQuery(this)[0].type === 'checkbox') {
            if (jQuery(this).prop('checked') === true) {
                jQuery('#resetFilters').prop('disabled', false).removeClass('btn-disabled');
                jQuery('#filter-btn').addClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'true');
                return false;
            } else {
                jQuery('#resetFilters').prop('disabled', true).addClass('btn-disabled');
                jQuery('#filter-btn').removeClass('has-filters-set');
                sessionStorage.setItem('HasDeckSearchFiltersSet', 'false');
            }
        }
    });

    const conditions = {
        deckName: deckname,
        doesContain: cardcodeurl,
        doesNotContain: notcardcodeurl,
        archetype: deckarchetype,
        author: deckauthor,
        fromDate: fromdate,
        toDate: todate,
        fromPrice: fromprice,
        toPrice: toprice,
        format: cardFormat,
        tournament: cardTournament,
        deckSize: decksize,
        sort: deckSort,
        range: deckrange,
        banlistCheck: jQuery('#filter-banlistcheck').is(':checked')
    };

    // Loader
    if (checkSearchSortFilterValues(conditions)) {
        jQuery('.searcher-placeholder-txt').html('<div class="loader" style="left: 0;"></div>');

        // Clear grid of decks and remove load more button when search/sort/filters have a value
        //jQuery('.deck-list').empty();

        // remove .deck_article-card-container from every .deck-placeholder inside .deck-list
        document.querySelectorAll('.deck-list > .deck-placeholder').forEach(placeholder => placeholder.innerHTML = '');

        // remove every .deck_article-card-container inside .deck-list that was appended by the Load More button
        document.querySelectorAll('.deck-list > .deck_article-card-container').forEach(deck => deck.remove());

        jQuery('#deckPaging').empty();

        // Reset offset and page number from sessionStorage
        if (typeof(Storage) !== 'undefined') {
            sessionStorage.setItem('DeckSearchOffset', 0);
            sessionStorage.setItem('DeckSearchPageNumber', 1);
        }
    } else {
        //jQuery('.deck-list').html('<div class="loader" style="left: 0;"></div>');

        // create a full width div container for the loader element's parent
        const loaderContainer = document.createElement('div');
        loaderContainer.id = 'loader';
        loaderContainer.className = 'd-flex justify-content-center w-100';

        // create the loader element itself
        const loaderDiv = document.createElement('div');
        loaderDiv.className = 'loader';
        loaderDiv.style.left = 0;

        loaderContainer.appendChild(loaderDiv); // append the loader element to its parent full width div container
        latestDecks.appendChild(loaderContainer); // append the loader's container element to .deck-list

        // Bring back placeholder text
        let placeholderTxt = `
            <span class="text-center bottom-pane-subtitle">Search the Yu-Gi-Oh! Deck Repository from YGOPRODeck</span>
            <div class="d-flex bottom-pane-extra-links">
                <div class="text-center" style="padding: 6px 12px;">
                    <a href="/tournaments/"><i class="fa-regular fa-trophy"></i> Tournaments</a>
                </div>
                <div class="text-center" style="padding: 6px 12px;">
                    <a href="/category/top-decks/"><i class="fa-solid fa-chart-line"></i> Top Decks</a>
                </div>
            </div>
        `;
        jQuery('.searcher-placeholder-txt').html(placeholderTxt);
    }

	//Limit
	apiurl+= "&limit=20";

    checkFilters();

	//Offset
	apiurl+= "&offset="+offset;

    // Remove class and empty grid of decks container
    jQuery('.deck-searcher-bottom-pane').removeClass('search-filter-results');
    jQuery('.searcher-placeholder-txt').removeClass('no-padding');
    jQuery('.grid-of-decks').empty();

    // Remove pagination if the search/sort/filters don't have a value
    if (!checkSearchSortFilterValues(conditions)) {
        jQuery('#pagination-container').empty();
    }

    //Get Decks
    jQuery.getJSON(apiurl,
    function(val) {
        //jQuery('.deck-list').empty();

        // if .deck-list contains the loader, remove it
        const loader = document.getElementById('loader');
        if (latestDecks.contains(loader)) { loader.remove() }

        if ( !val.error ) {
            // Enable previous button if disabled and not first page
            if (jQuery('.nextDeck').find('span.page-link').length && offset !== 0) {
                jQuery('.nextDeck').removeClass("disabled").attr('onclick', "nextDeck()").empty().append('<a class="page-link pagelink-btn" href="#">Previous</a>');
            }

            //Enable next button if Disabled
            jQuery('.prevDeck').removeClass("disabled").attr('onclick', "previousDeck()");
            if (jQuery('.prevDeck').find('span.page-link').length) {
                jQuery('.prevDeck').empty().append('<a class="page-link pagelink-btn" href="#">Next</a>');
            }
            const count = val.length;

            // Clear the bottom part of the card container, append pagination if there's none initially, and set page number in pagination if the search/sort/filters have a value.
            // Else, bring back the load more button
            if (checkSearchSortFilterValues(conditions)) {
                jQuery('.deck-searcher-bottom-pane').addClass('search-filter-results');
                jQuery('.searcher-placeholder-txt').empty().addClass('no-padding');

                let paginationElement = document.getElementById('pagination-elem');
                let pagination = `
                    <nav id="pagination-elem" class="w-100" aria-label="Deck Search Page Navigation" style="margin-top: 22px;">
                        <ul class="pagination justify-content-center">
                            <li class="page-item nextDeck disabled"><span class="page-link pagelink-btn">Previous</a></li>
                            <li class="page-item active" aria-current="page"><span class="page-link"><span id="page-num">1</span><span class="sr-only"> (current)</span></span></li>
                            <li class="page-item prevDeck" onclick="previousDeck()"><a class="page-link pagelink-btn" href="#">Next</a></li>
                        </ul>
                    </nav>
                `;
                let paginationContainer = document.getElementById('pagination-container');
                if (!paginationContainer.contains(paginationElement)) {
                    jQuery('#pagination-container').append(pagination);
                }

                if (typeof(Storage) !== 'undefined') {
                    let pageNumber = parseInt(sessionStorage.getItem('DeckSearchPageNumber'));
                    jQuery('#page-num').text(pageNumber);
                }

                renderGridDeckCards('.grid-of-decks', val, {widths: {w1: '33.33333333%', w2: '50%'}});
            } else {
                let loadMoreBtn = `
                    <button id="loadMore" class="btn d-flex align-items-center light-btn" type="button" onclick="loadMore()" style="gap: 4px;">
                        <span id="loadMoreText">Load More</span>
                        <div id="loadMoreIcon" class="d-flex justify-content-center align-items-center" style="width: 24px; height: 24px;">
                            <i class="fa-solid fa-chevron-right"></i>
                        </div>
                    </button>
                `;
                jQuery('#deckPaging').html(loadMoreBtn);

                //renderGridDeckCards('.deck-list', val, {widths: {w1: '33.33333333%', w2: '50%'}});

                // render a deck grid item inside its respective placeholder
                val.forEach((item, idx) => renderGridDeckCards(`[data-counter=${idx + 1}]`, [item], {widths: {w1: '100%', w2: '100%'}}));
            }

            //Featured Deck Builders
            if(featured == 1){
                jQuery('.featured-list').html('<i class="fas fa-star"></i> Featured Deck Builders: ');
                jQuery.each( featuredNames, function( key, value ) {
                    jQuery('.featured-list').append("<span class='featured-name'>"+value+"</span>");
                });
            }

            if(count < 20){
                jQuery('.prevDeck').addClass("disabled").removeAttr("onclick").off("click").empty().append('<span class="page-link pagelink-btn">Next</a>');
            }
        }else{
            jQuery('#pagination-container').empty();
            jQuery('.searcher-placeholder-txt').html('<span class="not-found"><i class="fas fa-ban"></i> No Decks Found.</span>');
            jQuery('.prevDeck').addClass("disabled").prop("onclick", null).off("click").empty().append('<span class="page-link pagelink-btn">Next</a>');

            //Enable Next Deck
            jQuery('.nextDeck').removeClass("disabled").attr('onclick', "nextDeck()");
        }

    }).fail(function() {
        offset = 0;
        jQuery('#pagination-container').empty();
        jQuery('.searcher-placeholder-txt').html('<span class="not-found"><i class="fas fa-ban"></i> No Decks Found.</span>');
		jQuery('.prevDeck').addClass("disabled").prop("onclick", null).off("click").empty().append('<span class="page-link pagelink-btn">Next</a>');
		jQuery('.nextDeck').addClass("disabled").prop("onclick", null).off("click");
	});

    //Retreive GET query string params for Query String URL
    var searchParams = new URLSearchParams(apiurl);
    var apiparams = "#";
    //Foreach query string param
    searchParams.forEach(function(value, key) {
      if(key !== "/api/decks/getDecks.php?" && key !== "limit"){
          apiparams += "&"+key+"="+encodeURIComponent(value);

      }
    });
    if(apiparams != "#"){
       history.pushState('', 'Deck Search - YGOPRODeck', '/deck-search/'+apiparams);
    }

}

function previousDeck(){
    if (window.innerWidth <= 982) {
        window.scrollTo({ top: 170, behavior: 'smooth' });
    }

    let pageOffset = 0;
    let pageNumber = 1;

    if (typeof(Storage) !== 'undefined') {
        pageOffset = parseInt(sessionStorage.getItem('DeckSearchOffset'));
        pageNumber = parseInt(sessionStorage.getItem('DeckSearchPageNumber'));
    }

    pageOffset += 20;
    searchDecks(pageOffset);

    pageNumber += 1;
    jQuery('#page-num').text(pageNumber);

    if (typeof(Storage) !== 'undefined') {
        sessionStorage.setItem('DeckSearchOffset', pageOffset);
        sessionStorage.setItem('DeckSearchPageNumber', pageNumber);
    }
    event.preventDefault();
}

function nextDeck(){
    if (window.innerWidth <= 982) {
        window.scrollTo({ top: 170, behavior: 'smooth' });
    }
    
    let pageOffset = 0;
    let pageNumber = 1;

    if (typeof(Storage) !== 'undefined') {
        pageOffset = parseInt(sessionStorage.getItem('DeckSearchOffset'));
        pageNumber = parseInt(sessionStorage.getItem('DeckSearchPageNumber'));
    }

    pageOffset -= 20;
    searchDecks(pageOffset);

    pageNumber -= 1;
    jQuery('#page-num').text(pageNumber);

    if (typeof(Storage) !== 'undefined') {
        sessionStorage.setItem('DeckSearchOffset', pageOffset);
        sessionStorage.setItem('DeckSearchPageNumber', pageNumber);
    }
    event.preventDefault();
}

function resetFilters(){
    jQuery('input[type=text],input[type=number],input[type=date]').val('');
    jQuery('select').prop('selectedIndex', 0);
    jQuery('input[type=checkbox]').prop('checked', false); //Uncheck all checkboxes
    searchDecks(0);
}

//Code Inputs
function addCodeInput(number){
    if(number == 2){
        jQuery('#DSPcardCode2, #addCodeButton3').css('display', 'block');
        jQuery('#addCodeButton2').hide();
    }else{
        jQuery('#DSPcardCode3').css('display', 'block');
        jQuery('#addCodeButton3').hide();
    }
}

// Code Input (for "doesn't contain card" inputs)
function addCodeInputx(number) {
    if(number == 2){
        jQuery('#DSPcardCode2x, #addCodeButton3x').css('display', 'block');
        jQuery('#addCodeButton2x').hide();
    }else{
        jQuery('#DSPcardCode3x').css('display', 'block');
        jQuery('#addCodeButton3x').hide();
    }
}

//Clear Individual Filter
function clearFilter(filter){

    if(filter == "-Date" || filter == "+Date"){
        jQuery('[data-filter="'+filter+'"]').val("");
    }else if(filter == "-Price" || filter == "+Price"){
        jQuery('[data-filter="'+filter+'"]').val("");
    }else if(filter == "Banlist"){
        jQuery( "#filter-banlistcheck" ).prop( "checked", false );
    }else{
        jQuery('[data-filter="'+filter+'"]').prop('selectedIndex',0);
    }

    searchDecks(0);
}

//Check filters for filter message above search button
function checkFilters(){

    var filtercount = 0;
    var filterhtml = '';
    //Selects
    jQuery.each(jQuery('#deck-form select'), function(ind,val){
      if(jQuery(this).find('option:selected').index() !== 0 && jQuery(this).data("filter") !== "Banlist"){
          filtercount++;
          var datafilter = jQuery(this).data("filter");
          filterhtml += '<span class="dsp-filteritem"><span class="dsp-filter-label" title="Clear '+datafilter+' filter" onClick="clearFilter(\''+datafilter+'\')">'+jQuery(this).data("filter")+':</span> '+jQuery(this).find('option:selected').val()+'</span>,';
      }
    });
    //Dates
    jQuery.each(jQuery('#deck-form input[type="date"]'), function(ind,val){
      if(jQuery(this).val()){
          filtercount++;
          var datafilter = jQuery(this).data("filter");
          filterhtml += '<span class="dsp-filteritem"><span class="dsp-filter-label" title="Clear '+datafilter+' filter" onClick="clearFilter(\''+datafilter+'\')">'+jQuery(this).data("filter")+':</span> '+jQuery(this).val()+'</span>,';
      }
    });
    //Prices
    jQuery.each(jQuery('#deck-form input[type="number"]'), function(ind,val){
      if(jQuery(this).val()){
          filtercount++;
          var datafilter = jQuery(this).data("filter");
          filterhtml += '<span class="dsp-filteritem"><span class="dsp-filter-label" title="Clear '+datafilter+' filter" onClick="clearFilter(\''+datafilter+'\')">'+jQuery(this).data("filter")+':</span> '+jQuery(this).val()+'</span>,';
      }
    });
    //Banlist
    if (jQuery('#filter-banlistcheck').is(':checked')) {
        filtercount++;
        var datafilter = jQuery('#filter-banlistcheck').data("filter");
        filterhtml += '<span class="dsp-filteritem"><span class="dsp-filter-label" title="Clear '+datafilter+' filter" onClick="clearFilter(\''+datafilter+'\')">'+jQuery('#filter-banlistcheck').data("filter")+':</span> '+jQuery('#filter-banlistcheck').val()+'</span>,';
    }

    //Remove last comma
    filterhtml = filterhtml.replace(/(\s*,?\s*)*$/, "");

    jQuery('.dsp-currentfilterslist').html(filterhtml);

    if(filtercount > 0){
        jQuery('#filter-area').addClass("d-block");
        jQuery('#filter-area').removeClass("d-none");
    }else{
        jQuery('#filter-area') .addClass("d-none");
        jQuery('#filter-area').removeClass("d-block");
    }

}

// toggle filter panel display
function toggleFilterPanel() {
    $('body').toggleClass("filter-panel-open-mobile");
    jQuery('#filter-panel').slideToggle(300);
}

// hide filter panel when clicking/tapping anywhere outside it
jQuery(document).on('mousedown', function (e) {
    let divToHide = document.getElementById('filter-panel');
    const suggestions = [...document.querySelectorAll('div.autocomplete-suggestions')];
    if (!divToHide.contains(e.target) && e.target.id !== 'filter-btn' && !suggestions.some(suggestion => suggestion.contains(e.target))) {
        hideFilterPanel();
    }
});

// hide filter panel
function hideFilterPanel() {
    jQuery('#filter-panel').slideUp(300);
    $('body').removeClass("filter-panel-open-mobile");
    if (sessionStorage.getItem('HasDeckSearchFiltersSet') === 'true') {
        jQuery('#filter-btn').addClass('has-filters-set');
    } else {
        jQuery('#filter-btn').removeClass('has-filters-set');
    }
}

// hide filter panel when tapping anywhere outside the filter panel (basically the shadow, only in mobile view)
jQuery('#filter-panel').on('click', function (e) {
    if (e.target.id === 'filter-panel') {
        hideFilterPanel();
    }
});

function loadMore() {
    deckGrid += 20;
    var apiurl = "/api/decks/getDecks.php?";
    apiurl+= "&offset="+deckGrid;

    // disable load more button when loading
    jQuery('#loadMoreText').text('Loading...');
    jQuery('#loadMoreIcon').html('<div class="spinner-border spinner-border-sm text-primary" role="status"></div>');
    jQuery('#loadMore').addClass("disabled").prop('disabled', true).removeAttr("onclick").off("click");

    jQuery.getJSON(apiurl, function (val) {
        if (!val.error) {
            renderGridDeckCards('.deck-list', val, {clearContainer: false, widths: {w1: '33.33333333%', w2: '50%'}});

            // enable load more button again
            jQuery('#loadMore').removeClass("disabled").prop('disabled', false).attr('onclick', 'loadMore()');
        } else {
            jQuery('.deck-list').append('<span class="not-found w-100 text-center"><i class="fas fa-ban"></i> Can\'t Load More.</span>');
        }

        // revert to the initial text content of load more button
        jQuery('#loadMoreText').text('Load More');
        jQuery('#loadMoreIcon').html('<i class="fa-solid fa-chevron-right"></i>');
    }).fail(function () {
        jQuery('.deck-list').append('<span class="not-found w-100 text-center"><i class="fas fa-ban"></i> Error Loading Additional Decks.</span>');
        jQuery('#loadMoreText').text('Load More');
        jQuery('#loadMoreIcon').html('<i class="fa-solid fa-chevron-right"></i>');
    });
}
